   "source": [
    "threshold = 1.5\n",
    "inputs = [1, 0, 1, 0, 1]\n",
    "weights = [0.7, 0.6, 0.5, 0.3, 0.4]\n",
    "def perceptron(): \n",
    "    sum = 0\n",
    "    for i in range(len(inputs)):\n",
    "      sum += inputs[i] * weights[i]\n",
    "\n",
    "    activate = (sum > threshold)"
   ]
  },
